{
  "module_code": "M11",
  "vector": 1,
  "name": "Funnel Nota Doi (Make/Zapier)",
  "purpose": "Orchestrare lead → quiz → VIP → curs cu webhooks, retries și DLQ robuste.",
  "tags": ["funnel", "automation", "make", "zapier"],
  "status": "stable",
  "semver": "0.1.0",
  "inputs": {
    "sevenD_contract": { "required": ["domain", "output"], "output_must_be_one_of": ["bundle", "json", "md"] },
    "schema": {
      "type": "object",
      "required": ["trigger", "ab_points", "tagging"],
      "properties": {
        "trigger": { "type": "string", "minLength": 3 },
        "ab_points": { "type": "array", "minItems": 2, "items": { "type": "string" } },
        "tagging": { "type": "array", "minItems": 1, "items": { "type": "string" } }
      }
    },
    "example": { "trigger": "form_submit", "ab_points": ["quiz", "email"], "tagging": ["VIP", "segment:lead"] }
  },
  "outputs": {
    "artifact_type": "bundle",
    "fields": [
      { "name": "blueprints_yaml", "type": "string", "required": true },
      { "name": "logs", "type": "json", "required": true }
    ]
  },
  "kpi": { "score_min": 80 },
  "tests": [ { "id": "M11-T1", "name": "Generează blueprint și logs", "input": { "custom": { "trigger": "t", "ab_points": ["a","b"], "tagging": ["x"] } }, "assert": { "schema_ok": true, "output_spec_complete": true, "kpi_min": 80 } } ],
  "guardrails": { "no_unfounded_claims": true, "privacy_safe": true, "respect_style": true },
  "deps": [],
  "evaluation_hooks": ["clarity", "execution", "ambiguity", "business_fit"],
  "telemetry_keys": ["run_id", "model", "tokens_in", "tokens_out", "scores", "policy_hits"]
}


