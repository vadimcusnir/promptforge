{
  "module_code": "M31",
  "vector": 6,
  "name": "Closed‑Loop Telemetry",
  "purpose": "ETL→warehouse→re‑antrenare: definește pipelines, queries și alerts pentru feedback închis pe prompts și agenți.",
  "tags": ["etl", "warehouse", "telemetry"],
  "status": "stable",
  "semver": "0.1.0",
  "inputs": {
    "sevenD_contract": { "required": ["domain", "output"], "output_must_be_one_of": ["bundle", "json"] },
    "schema": {
      "type": "object",
      "required": ["events", "schemas", "slos"],
      "properties": {
        "events": { "type": "array", "minItems": 3, "items": { "type": "string" } },
        "schemas": { "type": "object" },
        "slos": { "type": "object" }
      }
    },
    "example": { "events": ["run.started","policy.hit","run.finished"], "schemas": {}, "slos": { "freshness_min": 15 } }
  },
  "outputs": {
    "artifact_type": "bundle",
    "fields": [
      { "name": "pipelines", "type": "json", "required": true },
      { "name": "queries", "type": "array", "required": true },
      { "name": "alerts", "type": "json", "required": true }
    ]
  },
  "kpi": { "score_min": 80 },
  "tests": [ { "id": "M31-T1", "name": "Pipelines + alerts", "input": { "custom": { "events": ["a","b","c"], "schemas": {}, "slos": {} } }, "assert": { "schema_ok": true, "output_spec_complete": true, "kpi_min": 80 } } ],
  "guardrails": { "privacy_safe": true, "respect_style": true, "no_unfounded_claims": true },
  "deps": [],
  "evaluation_hooks": ["clarity", "execution", "ambiguity", "business_fit"],
  "telemetry_keys": ["run_id", "model", "tokens_in", "tokens_out", "scores", "policy_hits"]
}


