{
  "module_code": "M33",
  "vector": 6,
  "name": "Lead Scoring Model",
  "purpose": "Scor bazat pe intenție semantică (NLP) + evenimente; explicabil și rutabil.",
  "tags": ["scoring", "nlp", "routing"],
  "status": "stable",
  "semver": "0.1.0",
  "inputs": { "sevenD_contract": { "required": ["domain", "output"], "output_must_be_one_of": ["bundle", "json"] }, "schema": { "type": "object", "required": ["features", "labels", "threshold"], "properties": { "features": { "type": "array", "items": { "type": "string" }, "minItems": 3 }, "labels": { "type": "array", "items": { "type": "string" }, "minItems": 2 }, "threshold": { "type": "number", "minimum": 0, "maximum": 1 } } }, "example": { "features": ["intent","events"], "labels": ["SQL","nonSQL"], "threshold": 0.7 } },
  "outputs": { "artifact_type": "bundle", "fields": [ { "name": "score", "type": "json", "required": true }, { "name": "explain", "type": "markdown", "required": true }, { "name": "route", "type": "json", "required": false } ] },
  "kpi": { "score_min": 80 },
  "tests": [ { "id": "M33-T1", "name": "Score + explain", "input": { "custom": { "features": ["a","b","c"], "labels": ["x","y"], "threshold": 0.5 } }, "assert": { "schema_ok": true, "output_spec_complete": true, "kpi_min": 80 } } ],
  "guardrails": { "privacy_safe": true, "respect_style": true, "no_unfounded_claims": true },
  "deps": [],
  "evaluation_hooks": ["clarity", "execution", "ambiguity", "business_fit"],
  "telemetry_keys": ["run_id", "model", "tokens_in", "tokens_out", "scores", "policy_hits"]
}


